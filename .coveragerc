[run]
source = .
omit = 
    tests/*
    */test_*.py
    */__pycache__/*
    */site-packages/*
    .git/*
    ronin_agent.egg-info/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstract
precision = 2
show_missing = True

[html]
directory = htmlcov